{
    "categories": [
        {
            "title": "## 🚀 Added",
            "labels": ["feature", "feat", "add", "added"]
        },
        {
            "title": "## 🐛 Fixed",
            "labels": ["fix", "fixed"]
        },
        {
            "title": "## 🧪 Tests",
            "labels": ["test", "tested"]
        },
        {
            "title": "## ❌ Removed",
            "labels": ["del", "deleted", "removed"]
        },
        {
            "title": "## 📖 Changed",
            "labels": ["updated", "update", "changed", "change"]
        },
        {
            "title": "## ⛔ Deprecated",
            "labels": ["deprecated"]
        },
        {
            "title": "## 🚨 Security ",
            "labels": ["security"]
        },
        {
            "title": "## 📦 Dependencies",
            "labels": ["dependencies", "packages"]
        },
        {
            "title": "## 💬 Other",
            "labels": []
        }
    ],
    "pr_template": "${{TITLE}}",
    "label_extractor": [
        {
            "pattern": "(.+?)([:]?) (.+)",
            "target": "$1"
        }
    ],
    "transformers": [
        {
            "pattern": "(.+?)([:]?) (.+)",
            "target": "- $3"
        }
    ],
    "max_pull_requests": 1000,
    "max_back_track_time_days": 1000
}
